<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soecode.lyf.dao.OrderDao">
	<!-- 目的：为dao接口方法提供sql语句配置 -->

	<select id="queryOrderAll" resultType="Order">
		SELECT
			user_id,
			seller_id,
			md_id,
			order_create_time,
			order_number,
			order_total_price,
			order_single_number
		FROM
			order_detail
	</select>

	<insert id="addOrder">
		INSERT INTO
			order_detail (user_id,seller_id,md_id,order_create_time,order_number,order_single_number)
		VALUES
			(#{userid},#{sellerid},#{mdid},#{order_create_time},#{order_number},#{order_single_number})
	</insert>


</mapper>